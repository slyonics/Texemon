<?xml version="1.0" encoding="utf-8"?>
<View>

	<Panel Name="EnemyPanel" Style="Bind PlayerProfile.FrameStyle" Anchor="$CenterX,$CenterY" Bounds="Bind EnemyWindow" InnerMargin="3,3,3,3" TransitionIn="Expand">
		<Image RenderBinding="BackgroundRender" Alignment="Stretch" Depth="1.0"/>
		
	</Panel>

	<Panel Style="Bind PlayerProfile.WindowStyle" Anchor="$CenterX,$CenterY" Bounds="Bind PlayerWindow" InnerMargin="3,3,3,3" TransitionIn="Expand">
		<DataGrid Items="Bind PlayerProfile.Party" Bounds="0,0,120,80">
			<Label Text="Bind DataGrid.Name" Alignment="Cascading">
				
			</Label>
		</DataGrid>
	</Panel>
	
</View>