<?xml version="1.0" encoding="utf-8"?>
<View>

	<Panel Name="EnemyPanel" Style="Bind PlayerProfile.FrameStyle" Anchor="$CenterX,$CenterY" Bounds="Bind EnemyWindow" InnerMargin="3,3,3,3" TransitionIn="Expand">
		<Image RenderBinding="BackgroundRender" Alignment="Stretch" Depth="1.0"/>
		
	</Panel>

	<Panel Style="Bind PlayerProfile.WindowStyle" Anchor="$CenterX,$CenterY" Bounds="Bind PlayerWindow" InnerMargin="8,8,8,8" TransitionIn="Expand">
		<DataGrid Items="Bind PlayerProfile.Party" Alignment="Stretch">
			<Panel Alignment="Vertical" Bounds="0,0,112,20">
				<Label Text="Bind DataGrid.Name">
				
				</Label>
			</Panel>
		</DataGrid>
	</Panel>
	
</View>